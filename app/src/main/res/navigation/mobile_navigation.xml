    <fragment
        android:id="@+id/nav_tools"
        android:name="com.example.paseandopaseador.ui.tools.ToolsFragment"
        android:label="@string/paseando"
        tools:layout="@layout/fragment_tools" />

    <fragment
        android:id="@+id/nav_acerca"
        android:name="com.example.paseandopaseador.ui.share.ShareFragment"
        android:label="@string/paseando"
        tools:layout="@layout/fragment_share" />

    <fragment
        android:id="@+id/nav_send"
        android:name="com.example.paseandopaseador.ui.send.SendFragment"
